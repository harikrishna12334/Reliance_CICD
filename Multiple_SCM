pipeline {
    agent any
    parameters {
        gitParameter branchFilter: 'origin.*/(.*)', defaultValue: 'master', name: 'BRANCH_A', type: 'PT_BRANCH', useRepository: '.*Reliance_CICD.git'
        gitParameter branchFilter: 'origin.*/(.*)', defaultValue: 'master', name: 'BRANCH_B', type: 'PT_BRANCH', useRepository: '.*Reliance_CICD_Multipe_SCM.git'
        
    }
    stages {
        stage('Example') {
            steps {
                git branch: "${params.BRANCH_A}", url: 'https://github.com/harikrishna12334/Reliance_CICD.git'
                git branch: "${params.BRANCH_B}", url: 'https://github.com/harikrishna12334/Reliance_CICD_Multipe_SCM.git'
            }
        }
    }
}
