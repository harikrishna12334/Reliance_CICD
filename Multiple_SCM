pipeline {
    agent any
    parameters {
        gitParameter branchFilter: 'origin.*/(.*)', defaultValue: 'master', name: 'BRANCH_A', type: 'PT_BRANCH', useRepository: '.*Reliance_CICD.git'
       
        
    }
    stages {
        stage('Example') {
            steps {
                git branch: "${params.BRANCH_A}"
             
                
            }
        }
    }
}
